{
  "name": "be-ui",
  "version": "3.0.2",
  "packageManager": "pnpm@6.32.4",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/baiwusanyu-c/Be-UI.git"
  },
  "workspaces": [
    "packages/*",
    "docs"
  ],
  "scripts": {
    "init": "pnpm install",
    "dev": "vite --host",
    "build": "rollup -c rollup.config.js && pnpm run release",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint:ts": "eslint ./packages --ext .vue,.js,.ts,.jsx,.tsx",
    "lint:ts-fix": "eslint --fix ./packages --ext .vue,.js,.ts,.jsx,.tsx",
    "docs:dev": "pnpm run -C docs docs:dev",
    "docs:build": "pnpm run -C docs docs:build",
    "docs:serve": "pnpm run -C docs docs:serve",
    "release": "cp package.json README.md dist",
    "prettier:fix": "prettier --write .",
    "prepare": "husky install"
  },
  "lint-staged": {
    "./packages --ext .vue,.js,.ts,.jsx,.tsx": [
      "pretty-quick --staged",
      "eslint --fix",
      "git add"
    ]
  },
  "pnpm": {
    "overrides": {
      "jsdom": "16.4.0"
    }
  },
  "engines": {
    "node": ">= 16"
  },
  "browserslist": [
    "> 1%",
    "not ie 11",
    "not op_mini all"
  ],
  "peerDependencies": {
    "vue": "^3.2.31"
  },
  "dependencies": {
    "@be-ui/docs": "workspace:^1.0.0",
    "@popperjs/core": "^2.11.4",
    "@vue/shared": "^3.2.23",
    "vite-dts": "^1.0.4",
    "vue": "^3.2.31"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/plugin-proposal-class-properties": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@pnpm/find-workspace-packages": "^4.0.0",
    "@pnpm/logger": "^4.0.0",
    "@pnpm/types": "^8.0.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^21.0.3",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-typescript": "^8.3.2",
    "@testing-library/jest-dom": "^5.14.1",
    "@types/fs-extra": "^9.0.13",
    "@types/gulp": "^4.0.9",
    "@types/jest": "^27.0.2",
    "@types/marked": "^2.0.2",
    "@types/node": "*",
    "@types/sass": "^1.43.1",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vitejs/plugin-vue-jsx": "^1.3.9",
    "@vue/babel-plugin-jsx": "^1.1.1",
    "@vue/compiler-sfc": "^3.0.5",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/test-utils": "^2.0.0-rc.18",
    "@vue/tsconfig": "^0.1.3",
    "@vue/vue3-jest": "^27.0.0",
    "babel-core": "^6.25.0 || ^7.0.0-0",
    "babel-jest": "^27.5.1",
    "esbuild": "*",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.5.0",
    "husky": "^7.0.4",
    "jest": "^27.0.2",
    "jest-environment-jsdom": "^27.0.2",
    "lint-staged": "^12.3.7",
    "prettier": "2.5.1",
    "pretty-quick": "v3.1.3 ",
    "rollup": "^2.70.2",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-dts": "^4.2.1",
    "rollup-plugin-esbuild": "^4.9.1",
    "rollup-plugin-scss": "3",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.50.1",
    "ts-jest": "27.1.4",
    "tslib": "^2.4.0",
    "typescript": "^4.6.3",
    "vite": "^2.9.5",
    "vite-plugin-inspect": "^0.4.3",
    "vite-plugin-pwa": "^0.11.13",
    "vite-plugin-style-import": "^2.0.0",
    "vite-plugin-windicss": "^1.8.4",
    "vue": "^3.2.31",
    "vue-tsc": "^0.34.10",
    "windicss": "^3.4.3"
  }
}
